INSERT INTO `pages` (`id`, `title`, `alias`, `status`, `content`, `parent_id`, `order`, `meta_title`, `meta_keywords`, `meta_description`) VALUES (1,'Giới thiệu','gioi-thieu',1,'<p>Năm 2004,&nbsp;<strong>C&ocirc;ng ty cổ phần đầu tư hạ tầng v&agrave; c&ocirc;ng tr&igrave;nh kiến tr&uacute;c H&agrave; Nội</strong>&nbsp;(<em>HATECO.,JSC</em>) được th&agrave;nh lập bởi những cổ đ&ocirc;ng c&oacute; nhiều kinh nghiệm v&agrave; th&agrave;nh c&ocirc;ng trong lĩnh vực đầu tư t&agrave;i ch&iacute;nh, quản l&yacute; đầu tư x&acirc;y dựng cơ bản dựa tr&ecirc;n nguy&ecirc;n tắc giữ vững&nbsp;<strong><a href=\"http://hateco.com.vn/gia-tri-cot-loi/\">tầm nh&igrave;n, sứ mệnh &amp; gi&aacute; trị cốt l&otilde;i</a></strong>&nbsp;đ&atilde; định hướng trong to&agrave;n doanh nghiệp.<br />\r\n<br />\r\nNg&agrave;nh nghề kinh doanh ch&iacute;nh: Đầu tư t&agrave;i ch&iacute;nh v&agrave; đầu tư kinh doanh bất động sản.<br />\r\n<br />\r\nVới lịch sử hơn mười năm h&igrave;nh th&agrave;nh v&agrave; ph&aacute;t triển, C&ocirc;ng ty&nbsp;<strong>HATECO</strong>&nbsp;đ&atilde; trải qua nhiều giai đoạn kh&oacute; khăn, thử th&aacute;ch nhưng với kinh nghiệm của l&atilde;nh đạo C&ocirc;ng ty c&ugrave;ng sức trẻ v&agrave; nhiệt huyết của tất cả CBNV, HATECO đ&atilde; ph&aacute;t triển mạnh mẽ cả về chất v&agrave; lượng.<br />\r\n<br />\r\nĐ&oacute;ng g&oacute;p v&agrave;o th&agrave;nh c&ocirc;ng của c&ocirc;ng ty ng&agrave;y h&ocirc;m nay l&agrave; c&ocirc;ng sức rất lớn của đội ngũ c&aacute;n bộ chuy&ecirc;n gia cao cấp đ&atilde; kinh qua h&agrave;ng loạt những vị tr&iacute; quan trọng trong những tập đo&agrave;n kinh tế, trực tiếp quản l&yacute; đầu tư, triển khai c&aacute;c dự &aacute;n trọng điểm trong hầu hết c&aacute;c lĩnh vực trọng yếu như t&agrave;i ch&iacute;nh ng&acirc;n h&agrave;ng, đầu tư x&acirc;y dựng, kinh doanh bất động sản. B&ecirc;n cạnh đ&oacute; phải kể đến sự năng động, s&aacute;ng tạo, của đội ngũ chuy&ecirc;n vi&ecirc;n, kỹ sư, kiến tr&uacute;c sư đầy nhiệt huyết trực tiếp thực hiện, cụ thể h&oacute;a những chiến lược, &yacute; tưởng đầu tư.<br />\r\n<br />\r\nThực hiện mục ti&ecirc;u ph&aacute;t triển bền vững, b&ecirc;n cạnh việc tập trung đầu tư ph&aacute;t triển những dự &aacute;n bất động sản 100% vốn&nbsp;<strong>HATECO</strong>, c&ocirc;ng ty ch&uacute;ng t&ocirc;i c&ograve;n tham gia g&oacute;p vốn, li&ecirc;n doanh, li&ecirc;n kết với h&agrave;ng loạt c&aacute;c đối t&aacute;c chiến lược như: Vietinbank, Đ&ocirc;ng &Aacute; bank, C&ocirc;ng ty cổ phần giải ph&aacute;p thanh to&aacute;n Việt Nam&hellip;.<br />\r\n<br />\r\nVới tổng số vốn đ&atilde; đầu tư; g&oacute;p vốn l&ecirc;n đến h&agrave;ng ng&agrave;n tỷ đồng, C&ocirc;ng ty&nbsp;<strong>HATECO&nbsp;</strong>đ&atilde; triển khai v&agrave; đưa v&agrave;o kinh doanh khai th&aacute;c h&agrave;ng loạt dự &aacute;n Khu đ&ocirc; thị mới, Khu du lịch sinh th&aacute;i; khu chung cư căn hộ cao cấp, khu trung t&acirc;m thương mại dịch vụ, văn ph&ograve;ng ....<br />\r\n<br />\r\nC&ocirc;ng ty&nbsp;<strong>HATECO</strong>&nbsp;th&agrave;nh lập năm 2004 với vốn điều lệ: 8.000.000.000 đồng (T&aacute;m tỷ đồng) đến năm 2011 vốn điều lệ l&agrave; 500.000.000.000 (Năm trăm tỷ đồng).<br />\r\n<br />\r\n<strong>C&Ocirc;NG TY CỔ PHẦN ĐẦU TƯ HẠ TẦNG &amp; C&Ocirc;NG TR&Igrave;NH KIẾN TR&Uacute;C H&Agrave; NỘI&nbsp;</strong>(t&ecirc;n gọi tắt l&agrave;:<em>HATECO</em>) được th&agrave;nh lập theo giấy ph&eacute;p đăng k&yacute; kinh doanh số 0103005863 ng&agrave;y 04/11/2004, thay đổi lần thứ 13 ng&agrave;y 31/3/2011.<br />\r\n<br />\r\nT&ecirc;n tiếng Anh:&nbsp;<strong><em>HANOI INFRASTRUCTURE AND ARCHITECHTURE INVESTMENT JOINT STOCK COMPANY</em></strong><br />\r\n<br />\r\nT&ecirc;n viết tắt:&nbsp;<strong><em>HATECO.,JSC</em></strong><br />\r\n<br />\r\nĐịa chỉ trụ sở ch&iacute;nh:<strong>&nbsp;L&ocirc; 19.42, Khu ĐTM Trần L&atilde;m, Phường Trần L&atilde;m, TP Th&aacute;i B&igrave;nh</strong><br />\r\n<br />\r\nĐiện thoại:&nbsp;<strong>0363.742.880</strong>; Fax:&nbsp;<strong>0363.742.880</strong><br />\r\n<br />\r\nChi nh&aacute;nh:&nbsp;<strong>Tầng 3 T&ograve;a nh&agrave; Housing - 299 Trung K&iacute;nh - Cầu Giấy - H&agrave; Nội</strong><br />\r\n<br />\r\nĐiện thoại:&nbsp;<strong>043.2595.272</strong>; Fax:&nbsp;<strong>043.2595.365</strong><br />\r\n<br />\r\nEmail:<strong>&nbsp;<em><a href=\"mailto:info@hateco.com.vn\">info@hateco.com.vn</a></em></strong>; Website:&nbsp;<strong><em><a href=\"http://www.hateco.com.vn/\" rel=\"nofollow\" target=\"_blank\">www.hateco.com.vn</a></em><br />\r\n<br />\r\nDự &aacute;n đ&atilde; ho&agrave;n th&agrave;nh:&nbsp;<a href=\"http://hateco.com.vn/tran-lam/\">Khu đ&ocirc; thị mới Trần L&atilde;m</a><br />\r\nDự &aacute;n đang triển khai:&nbsp;<a href=\"http://hateco.com.vn/hateco-hoang-mai/\">Chung cư Hateco Ho&agrave;ng Mai</a><br />\r\nDự &aacute;n sắp triển khai:&nbsp;<a href=\"http://hateco.com.vn/project/categories/du-an-4a-huynh-thuc-khang.5/\">Hateco 4A Huỳnh Th&uacute;c Kh&aacute;ng</a></strong></p>\r\n',0,1,'','từ khóa giới thiệu','mô tả giới thiệu'),(4,'Sản phẩm khuyến mãi','san-pham-khuyen-mai',0,'',3,2,'','',''),(5,'Sản phẩm nổi bật','san-pham-noi-bat',0,'',3,1,'','',''),(6,'Dự án','du-an',1,'',0,3,'','',''),(7,'Tin tức','tin-tuc',1,'',0,2,'','',''),(8,'Tuyển dụng','tuyen-dung',0,'',0,4,'','',''),(10,'Chính sách bảo hành','chinh-sach-bao-hanh',0,'',9,1,'','',''),(11,'Chính sách vận chuyển','chinh-sach-van-chuyen',0,'',9,2,'','',''),(12,'Thông tin thanh toán','thong-tin-thanh-toan',0,'',9,3,'','',''),(14,'Lịch sử hình thành','lich-su-hinh-thanh',1,'',1,2,'','',''),(15,'Tầm nhìn - Sứ mệnh - Giá trị cốt lõi','tam-nhin-su-menh-gia-tri-cot-loi',1,'',1,2,'','',''),(16,'Sơ đồ tổ chức','so-do-to-chuc',1,'',1,3,'','',''),(17,'Tin Thị Trường','tin-thi-truong',1,'',7,1,'','',''),(18,'Tin Doanh Nghiệp','tin-doanh-nghiep',1,'',7,2,'','',''),(19,'Tin Bất Động Sản','tin-bat-dong-san',1,'',7,3,'','',''),(20,'Tư vấn','tu-van',1,'',0,7,'','',''),(21,'Tư Vấn Nội Thất','tu-van-noi-that',0,'',20,1,'','',''),(22,'T