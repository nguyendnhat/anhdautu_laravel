INSERT INTO `posts` (`id`, `meta_title`, `meta_keywords`, `meta_description`, `title`, `description`, `content`, `alias`, `img`, `page_id`, `created_at`, `updated_at`, `source`, `investment_capital`, `status`, `large`, `finish`, `file`, `tong_quan`, `uu_dai`, `vi_tri`, `tien_ich`, `giai_phap`, `chinh_sach`, `address`, `price`, `phone`) VALUES (52,'Dự án Iris Garden | 【Giá tốt, ưu đãi nhất 】™‎','chung cư iris garden,dự án iris garden vimefulland,chung cư iris garden mỹ đình,iris garden vimefulland,chung cư iris,bán chung cư iris garden,chung cư iris garden trần hữu dực,chung cư trần hữu dực,chung cư iris mỹ đình,dự án iris garden mỹ đình,dự án iris mỹ đình,,dự án chung cư iris garden','Dự án Iris Garden - Chiết khấu lên đến 5,1%, Vay vốn 0% 12 tháng,thanh toán làm 8 đợt. Có phòng khám gia đình 24/24,nước uống tại vòi,đỗ xe thông minh,tách mỡ nước thải,bể bơi. Trọn chữ Tâm vẹn chữ Tín. Hỗ trợ 24/7. Cam kết không bán chênh. Kí HĐMB trực tiếp CĐT.','Iris Garden','<ul>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>T&ecirc;n thương mại:</strong>&nbsp;Chung cư&nbsp;<strong>Iris&nbsp;Garden</strong></span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;T&ecirc;n dự &aacute;n:</strong>&nbsp;Khu nh&agrave; ở cao tầng dịch vụ văn ph&ograve;ng</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;Vi tr&iacute;:</strong>&nbsp;&nbsp;30 Trần Hữu Dực&nbsp;&ndash; K2, phường Cầu Diễn, quận Nam Từ Li&ecirc;m, H&agrave; Nội</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;Chủ đầu tư:&nbsp;</strong>C&ocirc;ng ty cổ phần đầu tư x&acirc;y dựng Nhuệ Giang</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;Đơn vị ph&aacute;t triển:</strong>&nbsp;Vimefulland</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>Đơn vị thiết kế:</strong>&nbsp;NKB Archi</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;Đơn vị&nbsp;thi c&ocirc;ng:</strong>&nbsp;Ecoba Việt Nam</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;Đơn vị quản l&yacute;:&nbsp;</strong>Savills Việt Nam</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;Tổng diện t&iacute;ch đất:&nbsp;</strong>22.492m2</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;Diện t&iacute;ch x&acirc;y dựng:</strong>&nbsp;7.154m2</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;Tổng diện t&iacute;ch s&agrave;n x&acirc;y dựng:</strong>153.893m2 (kh&ocirc;ng bao gồm s&agrave;n tầng hầm)</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>Mật độ x&acirc;y dựng:</strong>&nbsp;31,8%<strong>&nbsp;</strong></span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>Quy m&ocirc;:</strong>&nbsp;5 to&agrave;&nbsp; &nbsp;I1A, I1B cao 29 tầng</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;I2, I3, I4 cao 25 tầng</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;2 tầng hầm để xe li&ecirc;n th&ocirc;ng c&aacute;c to&agrave;. Cam kết 1 chỗ đỗ &ocirc; t&ocirc;/căn hộ</span></span></li>\r\n	<li><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>&nbsp;Tổng số căn hộ:</strong>&nbsp;1.128 căn. 8 &ndash; 12 căn hộ/ s&agrave;n, 4 thang m&aacute;y + 1 thang h&agrave;ng</span></span></li>\r\n</ul>\r\n','','iris-garden','/uploads/images/info/T%E1%BB%95ng%20quan%20d%E1%BB%B1%20%C3%A1n%20iris%20garden%2004.jpg',6,'2018-10-15 06:39:58','2018-10-17 13:41:01','','30 Đỗ Đức Dục',1,'Quý III/ 2019',1,'','<p><strong>Dự &aacute;n&nbsp;Iris Garden</strong>&nbsp;l&agrave; tổ hợp căn hộ cao cấp tọa lạc tại tại trung t&acirc;m h&agrave;nh ch&iacute;nh, kinh tế năng động tại ph&iacute;a T&acirc;y H&agrave; Nội &ndash; tr&ecirc;n tuyến đường Trần Hữu Dực. Dự &aacute;n bao gồm 5 t&ograve;a&nbsp;<strong>chung cư</strong>&nbsp;dự kiến cung cấp ra thị trường 1.128 căn hộ v&agrave;o qu&yacute; III năm 2019.</p>\r\n\r\n<p><strong>T&ecirc;n dự &aacute;n</strong>: Chung cư Iris Garden</p>\r\n\r\n<p><strong>Vị tr&iacute;:</strong>&nbsp;Số 30 Trần Hữu Dực, Mỹ Đ&igrave;nh 2, quận Nam Từ Li&ecirc;m, H&agrave; Nội</p>\r\n\r\n<p><strong>Chủ đầu tư</strong>: Tập đo&agrave;n Vimefulland</p>\r\n\r\n<p><strong>Đơn vị thi c&ocirc;ng</strong>: ECOBA Việt Nam</p>\r\n\r\n<p><strong>Diện t&iacute;ch đất:</strong>&nbsp;22.492</p>\r\n\r\n<p><strong>Diện t&iacute;ch x&acirc;y dựng</strong>: 7.154</p>\r\n\r\n<p><strong>Mật độ x&acirc;y dựng</strong>: 32</p>\r\n\r\n<p><strong>Số lượng căn hộ:</strong>&nbsp;1.128</p>\r\n\r\n<p><strong>Ng&acirc;n h&agrave;ng bảo l&atilde;nh:</strong>&nbsp;Việt &Aacute; Bank</p>\r\n\r\n<p><strong>Quy m&ocirc;:</strong>&nbsp;Tổ hợp thương mại dịch vụ, biệt thự, liền kề v&agrave; khối nh&agrave; cao tầng, 5 t&ograve;a chung cư cao 25 &ndash; 29 tầng.</p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p style=\"text-align:center\"><iframe frameborder=\"0\" height=\"360\" src=\"https://www.youtube-nocookie.com/embed/Mc4EwRUixjY\" width=\"640\"></iframe></p>\r\n','<h4 style=\"text-align:center\"><span style=\"color:#ffffff\"><strong><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">Những ưu đ&atilde;i đặc biệt cho kh&aacute;ch h&agrave;ng</span></span></strong></span></h4>\r\n\r\n<p style=\"text-align:center\"><span style=\"color:#ffffff\"><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><img src=\"https://3.bp.blogspot.com/-YgmwdDil_WE/UZPhSg5SmSI/AAAAAAAADwI/hG538v_YjI0/s1600/gift-emoticon.png\" />&nbsp;Chiết khấu&nbsp;<strong>5,1%</strong>&nbsp;cho kh&aacute;ch h&agrave;ng&nbsp;<strong>thanh to&aacute;n sớm&nbsp;95%</strong>&nbsp;GTHĐ</span></span></span></p>\r\n\r\n<p style=\"text-align:center\"><span style=\"color:#ffffff\"><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><img src=\"https://3.bp.blogspot.com/-YgmwdDil_WE/UZPhSg5SmSI/AAAAAAAADwI/hG538v_YjI0/s1600/gift-emoticon.png\" />&nbsp;Chiết khấu&nbsp;<strong>2,2%</strong>&nbsp;cho kh&aacute;ch h&agrave;ng&nbsp;<strong>thanh to&aacute;n sớm 70%</strong>&nbsp;GTHĐ</span></span></span></p>\r\n\r\n<p style=\"text-align:center\"><span style=\"color:#ffffff\"><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><img src=\"https://3.bp.blogspot.com/-YgmwdDil_WE/UZPhSg5SmSI/AAAAAAAADwI/hG538v_YjI0/s1600/gift-emoticon.png\" />&nbsp;Chiết khấu&nbsp;<strong>1%</strong>&nbsp;cho kh&aacute;ch h&agrave;ng&nbsp;<strong>thanh to&aacute;n sớm 50%</strong>&nbsp;GTHĐ</span></span></span></p>\r\n\r\n<p style=\"text-align:center\"><span style=\"color:#ffffff\"><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><img src=\"https://3.bp.blogspot.com/-YgmwdDil_WE/UZPhSg5SmSI/AAAAAAAADwI/hG538v_YjI0/s1600/gift-emoticon.png\" />&nbsp;Hỗ trợ vay tới&nbsp;<strong>65%</strong>&nbsp;GTCH,&nbsp;<strong>l&atilde;i suất 0%</strong>&nbsp;trong v&ograve;ng&nbsp;<strong>12 th&aacute;ng</strong>&nbsp;(cho KH thanh to&aacute;n bằng vốn tự c&oacute; 30% GTCH)</span></span></span></p>\r\n\r\n<p style=\"text-align:center\"><img alt=\"\" src=\"/uploads/images/uu%20dai%20irris.png\" style=\"height:470px; width:560px\" /></p>\r\n\r\n<p style=\"text-align:center\">&nbsp;</p>\r\n','<p><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><em><strong>Iris Garden</strong></em>&nbsp;tọa lạc tại số 30 Trần Hữu Dực,&nbsp;<strong>quận Nam Từ Li&ecirc;m</strong>&nbsp;&ndash; con đường kết nối trung t&acirc;m Mỹ Đ&igrave;nh v&agrave; KĐT Xu&acirc;n Phương, đối di&ecirc;n với Cung điền kinh thể thao quốc gia. Sở hữu hạ tầng giao th&ocirc;ng thuận tiện, dự &aacute;n nối liền nhiều trục đường huyết mạch như: L&ecirc; Đức Thọ, H&agrave;m Nghi, Hồ T&ugrave;ng Mậu, đại lộ Thăng Long, đường v&agrave;nh đai 3&hellip;</span></span></p>\r\n\r\n<p style=\"text-align:center\"><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><img alt=\"Thông xe đường Trần hữu Dực\" src=\"/uploads/images/thong_xe_duong_Tran_Huu_Duc_1_RELS.jpg\" style=\"height:500px; width:750px\" /></span></span></p>\r\n\r\n<p style=\"text-align:center\"><span style=\"font-size:16px\">Th&ocirc;ng xe đường Trần Hữu Dực</span></p>\r\n\r\n<p style=\"text-align:center\"><img alt=\"Thông xe đường trần hữu dực\" src=\"/uploads/images/thong_xe_duong_Tran_Huu_Duc_8_NMPE.jpg\" style=\"height:500px; width:700px\" /></p>\r\n\r\n<p><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">Iris Garden thừa hưởng to&agrave;n bộ hệ thống tiện &iacute;ch ngoại khu từ khu vực&nbsp; Mỹ Đ&igrave;nh được quy hoạch th&agrave;nh Trung t&acirc;m kinh tế ch&iacute;nh trị mới của Thủ đ&ocirc;, &ldquo;mảnh đất v&agrave;ng ph&iacute;a T&acirc;y H&agrave; Nội&rdquo; nhanh ch&oacute;ng trở th&agrave;nh nơi hội tụ c&aacute;c cơ quan trọng yếu của Ch&iacute;nh phủ: Bộ C&ocirc;ng an, Tổng cục Hải quan, Bộ Nội vụ, Bộ T&agrave;i nguy&ecirc;n &ndash; M&ocirc;i trường, Bộ Khoa học &ndash; C&ocirc;ng nghệ, Bộ Ngoại giao, Kiểm to&aacute;n Nh&agrave; nước&hellip;</span></span></p>\r\n\r\n<ul>\r\n	<li>\r\n	<p><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>Trường học danh tiếng</strong>: THCS Đo&agrave;n Thị Điểm, THPT Lomonoxop, Việt &Uacute;c, Marie Curie, L&ecirc; Qu&yacute; Đ&ocirc;n, ĐH Sư Phạm, Quốc Gia, Học viện b&aacute;o ch&iacute; tuy&ecirc;n truyền, ĐH Thương Mại&hellip;</span></span></p>\r\n	</li>\r\n	<li>\r\n	<p><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>Trung t&acirc;m h&agrave;nh ch&iacute;nh văn h&oacute;a</strong>: Bộ ngoại giao, Trung t&acirc;m hội nghị Quốc Gia, Bộ nội vụ, Bộ t&agrave;i nguy&ecirc;n, Khu tổ hợp thể thao Mỹ Đ&igrave;nh&hellip;</span></span></p>\r\n	</li>\r\n	<li>\r\n	<p><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\"><strong>Y tế</strong>: Bệnh viện 198, Viện y học cổ truyền, Viện huyết học &amp; truyền m&aacute;u trung ương, Bệnh viện thể thao.</span></span></p>\r\n	</li>\r\n</ul>\r\n\r\n<p style=\"text-align:center\"><img alt=\"Vị trí chung cư Iris Garden\" src=\"/uploads/images/vi-tri-chung-cu-iris-garden-my-dinh-1024x620.jpg\" style=\"height:620px; width:1024px\" /></p>\r\n','<p><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">L&agrave; một sản phẩm bất động sản của thương hiệu Vimefulland,<strong> Iris Garden</strong> tu&acirc;n thủ nghi&ecirc;m ngặt Bộ quy chuẩn ứng dụng c&aacute;c ti&ecirc;u chuẩn về chăm s&oacute;c sức khỏe, y tế của <strong>Vimefulland</strong> với 4 ti&ecirc;u chuẩn ch&iacute;nh bao gồm: Ti&ecirc;u chuẩn về nguồn nước sạch, Ti&ecirc;u chuẩn kiểm so&aacute;t &ocirc; nhiễm, Ti&ecirc;u chuẩn ph&aacute;t triển sức khoẻ thể chất v&agrave; Ti&ecirc;u chuẩn ph&aacute;t triển sức khoẻ tinh thần.</span></span></p>\r\n\r\n<p><img alt=\"mặt bằng tiện ích dự án Iris Garden\" src=\"/uploads/images/20170821_MB%20tien%20ich.jpg\" style=\"height:884px; width:1204px\" /></p>\r\n\r\n<p><strong>IRIS GARDEN</strong> nổi l&ecirc;n như một h&ograve;n đảo xanh xinh đẹp, được &ocirc;m trọn bởi d&ograve;ng s&ocirc;ng Nhuệ vượng kh&iacute;, trong l&agrave;nh với nhiều lớp c&acirc;y xanh bao phủ</p>\r\n\r\n<p>L&agrave; dự &aacute;n duy nhất tại H&agrave; Nội được thiết kế theo kiến tr&uacute;c T&acirc;n cổ điển Ph&aacute;p nổi bật mang phong c&aacute;ch tinh tế, sang trọng, lịch l&atilde;m, theo đ&oacute;, mỗi ph&ograve;ng trong mỗi căn hộ đều ngập tr&agrave;n &aacute;nh s&aacute;ng v&agrave; kh&ocirc;ng kh&iacute; lưu th&ocirc;ng.</p>\r\n\r\n<p style=\"text-align:center\"><img alt=\"Phối cảnh nội thất dự án Iris Garden\" src=\"/uploads/images/ph%E1%BB%91i%20c%E1%BA%A3nh%20n%E1%BB%99i%20th%E1%BA%A5t%20iris%20garden00001.jpg\" style=\"height:640px; width:1024px\" /></p>\r\n\r\n<p>To&agrave; I2 ( CT2) với chỉ 8 căn/s&agrave;n v&agrave; c&oacute; tới 4 thang m&aacute;y, đ&aacute;m bảo l&uacute;c n&agrave;o thang m&aacute;y cũng sẵn s&agrave;ng phục vụ, mật độ si&ecirc;u tốt!!!</p>\r\n\r\n<p style=\"text-align:center\"><img alt=\"Sảnh hành lang dự án iris garden\" src=\"/uploads/images/T%E1%BB%95ng%20quan%20d%E1%BB%B1%20%C3%A1n%20iris%20garden%2002.jpg\" style=\"height:683px; width:1024px\" /></p>\r\n\r\n<p>Được trang bị hệ thống an ninh 3 lớp với c&aacute;c thiết bị hiện đại, đảm bảo kh&ocirc;ng gian sống an l&agrave;nh cho cư d&acirc;n</p>\r\n\r\n<p>L&agrave; một trong số &iacute;t c&aacute;c dự &aacute;n c&oacute; diện t&iacute;ch để xe rộng r&atilde;i với hệ thống 3 tầng hầm, Vimefulland cũng cam kết mỗi căn hộ sẽ c&oacute; 1 chỗ để xe &ocirc;t&ocirc; v&agrave; 2 xe m&aacute;y</p>\r\n\r\n<p style=\"text-align:center\"><img alt=\"Bãi đỗ xe an ninh dự án iris garden\" src=\"/uploads/images/ha-noi-de-xuat-chon-nha-thau-nikken-sekkei-nhat-ban-cho-du-an-bai-do-xe-ngam1494986941.jpg\" style=\"height:664px; width:1024px\" /></p>\r\n\r\n<p><strong>Iris Garden</strong> l&agrave; tổ hợp nh&agrave; ở kh&eacute;p k&iacute;n sang trọng bậc nhất, ti&ecirc;n phong &aacute;p dụng c&ocirc;ng nghệ v&igrave; sức khoẻ với:</p>\r\n\r\n<ul>\r\n	<li>\r\n	<p>C&ocirc;ng nghệ lọc nước k&eacute;p đạt ti&ecirc;u chuẩn uống trực tiếp tại v&ograve;i</p>\r\n	</li>\r\n	<li>\r\n	<p>Hệ thống t&aacute;ch mỡ từ nước &amp; chất thải đảm bảo đường ống th&ocirc;ng tho&aacute;ng, kh&ocirc;ng m&ugrave;i, &ocirc; nhiễm, chống thải ngược</p>\r\n	</li>\r\n	<li>\r\n	<p>Hệ thống gom r&aacute;c tập trung &amp; được vận chuyển bằng thang m&aacute;y ri&ecirc;ng c&aacute;ch hộ gđ tối thiểu 10m, vệ sinh 2 tiếng/lần</p>\r\n	</li>\r\n	<li>\r\n	<p>Trang bị k&iacute;nh 3 lớp cao cấp chống ồn, chống nhiệt v&agrave; c&aacute;c tia c&oacute; hại cho căn hộ</p>\r\n	</li>\r\n	<li>\r\n	<p>Vật liệu th&acirc;n thiện m&ocirc;i trường: Sơn sinh th&aacute;i, b&ecirc; t&ocirc;ng c&aacute;ch nhiệt&hellip;</p>\r\n	</li>\r\n	<li>\r\n	<p>&nbsp;</p>\r\n	</li>\r\n</ul>\r\n','<p><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">Giả sử kh&aacute;ch h&agrave;ng mua 1 căn hộ 2 ph&ograve;ng ngủ, 2WC gi&aacute; trị 1.88 tỷ đ&atilde; bao gồm VAT, nội thất cao cấp.</span></span></p>\r\n\r\n<h3><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">TIẾN ĐỘ THANH TO&Aacute;N</span></span></h3>\r\n\r\n<table style=\"width:100%\">\r\n	<thead>\r\n		<tr>\r\n			<th><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">STT</span></span></th>\r\n			<th><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">ĐỢT</span></span></th>\r\n			<th><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">TIẾN ĐỘ TT</span></span></th>\r\n			<th><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">TIẾN ĐỘ THI C&Ocirc;NG</span></span></th>\r\n		</tr>\r\n	</thead>\r\n	<tbody>\r\n		<tr>\r\n			<td><span style=\"font-size:16px\"><span style=\"font-family:Times New Roman,Times,serif\">1</span></span></td>\r\n			<td><strong><spa